# Agent Portal Environment Variables
# 이 파일을 복사하여 .env로 사용하세요: cp .env.example .env

# Database
MARIADB_ROOT_PASSWORD=rootpass
MARIADB_DATABASE=agent_portal
REDIS_PASSWORD=redispass

# MinIO
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=admin12345

# Observability
OBSERVABILITY_ENABLED=true
HELICONE_PROXY_BASE=/embed/helicone
LANGFUSE_PROXY_BASE=/embed/langfuse
SECURITY_PROXY_BASE=/embed/security

# Kong Admin
KONG_ADMIN_PROXY_BASE=/embed/kong-admin
KONG_ADMIN_INTERNAL_URL=http://konga:1337

# Default Workspace
DEFAULT_WORKSPACE_FILTER=ws_default

# Backend BFF (Frontend에서 사용)
VITE_BFF_BASE_URL=http://localhost:8000

# AgentOps Self-Hosted Configuration
# docs/AGENTOPS_SETUP.md 가이드 참고
AGENTOPS_API_KEY=  # 대시보드에서 생성 후 입력
AGENTOPS_API_ENDPOINT=http://agentops-api:8003
AGENTOPS_APP_URL=http://localhost:3006
AGENTOPS_EXPORTER_ENDPOINT=http://otel-collector:4318/v1/traces

